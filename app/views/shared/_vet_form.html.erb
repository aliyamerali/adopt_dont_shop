<%= form_with url: url, method: method, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :review_rating %>
  <%= f.number_field :review_rating %>

  <%= f.label :on_call %>
  <%= f.check_box :on_call, checked_value: on_call_value %>

  <%= f.submit %>
<% end %>
